<h3 class="mb-2">
  <%= post.title %>
</h3>
<div class="mb-2">
  <p class="text text-secondary mb-1"> <%= post.crawler_setting.name %> </p>
  <p ><%= link_to post.from, post.from %></p> 
</div>


  
  <% if current_user %>
    <%= render partial: 'favorites_button', locals: { post: post, user: current_user }%>
  <% end %>
  


<article style="font-size: 1.1em">
  <%== post.content %>
</article>

