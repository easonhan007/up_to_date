<div class="d-flex my-4 gap-4">
  <h3>Settings</h3>
  <%= link_to "New", new_setting_path, class: "btn btn-primary" %>
</div>

<div id="settings">
  <table class="table">
    <thead>
      <th>ID</th>
      <th>Name</th>
      <th>Value</th>
      <th>Operation</th>
    </thead>
    <tbody>
      <% @settings.each do |setting| %>
        <tr>
          <td><%= link_to setting.id, setting_path(setting) %></td>
          <td><%= setting.name%></td>
          <td><%= setting.value%></td>
          <td><%= link_to "Edit", edit_setting_path(setting)%></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>

